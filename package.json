{
  "name": "aw-stack-2",
  "version": "0.0.1",
  "description": "A aw-stack-2 is a Angular.js 2.0, Webpack, ES6 (ECMAScript 2015), TypeScript, Babel, Karma, Jasmine, Twitter Bootstrap, LESS , ESLint, LESSHint front-end stack.",
  "scripts": {
    "start": "lite-server -c bs-config.json",
    "dist:clean": "rimraf dist",
    "coverage:clean": "rimrag coverage",
    "bundle": "npm run dist:clean && webpack -d --watch",
    "build": "npm run dist:clean && webpack -p -d",
    "test:all": "karma start",
    "postinstall": "typings install",
    "lint:all": "tslint -c tslint.json 'src/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/hegdeashwin/aw-stack-2.git"
  },
  "keywords": [
    "angular2-webpack-stack",
    "front-end-stack",
    "angular2-stack"
  ],
  "author": "Ashwin Hegde",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hegdeashwin/aw-stack-2/issues"
  },
  "homepage": "https://github.com/hegdeashwin/aw-stack-2#readme",
  "dependencies": {
    "@angular/common": "2.1.1",
    "@angular/compiler": "2.1.1",
    "@angular/core": "2.1.1",
    "@angular/forms": "2.1.1",
    "@angular/http": "2.1.1",
    "@angular/platform-browser": "2.1.1",
    "@angular/platform-browser-dynamic": "2.1.1",
    "@angular/router": "3.1.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.25",
    "reflect-metadata": "^0.1.8"
  },
  "devDependencies": {
    "angular-mock": "^1.0.0",
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^2.2.4",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "css-loader": "^0.23.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.1.2",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-webpack": "^1.7.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
